{
  "compilerOptions": {
    "module": "ESNext",
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": ".",
    "outDir": "../dist",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "react",
    "esModuleInterop": true,
    "target": "es2021",
    "paths": {
      "*": ["node_modules/*"]
    }
  },
  "include": ["./src/**/*"]
}
