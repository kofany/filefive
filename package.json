    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.3",
    "@types/ramda": "^0.31.1",
    "@types/ssh2": "^1.15.5",
    "@types/whatwg-url": "^13.0.0",
    "@types/ws": "^8.18.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "jest": "^30.2.0",
