  "version": "1.8.0",
  "license": "GPL-3.0",
  "author": "Max Miroshnikov",
  "bin": {
    "f5": "dist/index.js"
  },
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miroshnikov/filefive.git"
  },
  "homepage": "https://github.com/miroshnikov/filefive#readme",
  "bugs": {
    "url": "https://github.com/miroshnikov/filefive/issues"
